{
  "name": "youtube-online-api",
  "version": "0.1.0",
  "repository": {
    "url": "https://github.com/sebastianwd/youtube-online-api"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/app.ts",
    "postinstall": "patch-package",
    "start": "node dist/app.js",
    "transpile": "babel --extensions .ts,.js ./src -d ./dist",
    "build": "npm run clean && npm run transpile",
    "clean": "rimraf dist",
    "update": "ncu -i"
  },
  "dependencies": {
    "@trpc/server": "^9.22.0",
    "axios": "^0.24.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.30",
    "pino": "^7.11.0",
    "trpc-playground": "^0.1.6",
    "user-agents": "^1.0.843",
    "youtube-dl-exec": "^2.0.11",
    "yt-search": "^2.10.3",
    "ytdl-core": "^4.9.1",
    "ytsr": "^3.5.3",
    "zod": "^3.15.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@babel/runtime-corejs3": "^7.15.4",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.177",
    "@types/node": "^17.0.25",
    "@types/user-agents": "^1.0.2",
    "@types/yt-search": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "pino-pretty": "^7.6.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "tiny-typed-emitter": "^2.1.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix"
  }
}
